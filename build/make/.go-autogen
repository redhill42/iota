#!/bin/bash

rm -rf autogen

cat > api/version_lib.go <<DVEOF
// +build autogen

// Package version is auto-generated at build-time
package api

// Default build-time variable for library-import
// This file is overridden on build with build-time informations.
const (
	Version   string = "$VERSION"
	GitCommit string = "$GITCOMMIT"
	BuildTime string = "$BUILDTIME"

	// APIVersion represents current REST API version
	APIVersion string = "1"
	// MinAPIVersion represents minimum REST API version supported
	MinAPIVersion string = "1"
)

// AUTOGENERATED FILE; see $BASH_SOURCE
DVEOF
